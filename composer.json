{
    "name": "mvdnbrk/warehouse-framework",
    "description": "Laravel Warehouse Framework",
    "keywords": [
        "wms",
        "laravel",
        "warehouse",
        "warehouse management system"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mark van den Broek",
            "email": "mvdnbrk@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/console": "^7.2 || ^8.0",
        "illuminate/contracts": "^7.2 || ^8.0",
        "illuminate/database": "^7.2 || ^8.0",
        "illuminate/queue": "^7.2 || ^8.0",
        "illuminate/support": "^7.2 || ^8.0",
        "mvdnbrk/gtin": "^2.6",
        "mvdnbrk/laravel-model-expires": "^1.6",
        "spatie/laravel-model-states": "^1.5",
        "staudenmeir/eloquent-has-many-deep": "^1.13"
    },
    "require-dev": {
        "nunomaduro/collision": "^4.2",
        "orchestra/testbench": "^5.4 || ^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Just\\Warehouse\\WarehouseServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Just\\Warehouse\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Just\\Warehouse\\Tests\\": "tests"
        },
        "classmap": [
            "database/model-factories"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
